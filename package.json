    }
  },
  "files": [
    "dist/",
    "src/",
    "examples/",
    "lib/"
  ],
  "bin": {
    "stockfishjs": "./stockfishjs"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
